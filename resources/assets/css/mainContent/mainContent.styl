.MainContent
  border:2px solid black
  display: flex
  justify-content: space-around
  align-items: center
  min-height: 1100px

  &-category
    border:1px solid yellow
    display:flex
    justify-content: space-around
    align-items: flex-start
    flex-direction: row

    &-categoryMenu
      flex: 0 0 266px
      height: 578px
      background: #FFFFFF
      box-shadow: 0 16px 34px 7px rgba(121,121,121,0.10)
      border-radius: 4px

    &-categoryTitle
      font-size: 11px
      color: #A3A3A3  
      font-size: 11px
      margin-left: 20px

    &-resultContent
      border:1px solid green
      display:flex
      justify-content: flex-start
      align-items: flex-start
      flex-direction: column

    &-resultTitle
      font-family: 'SanFranciscoTextRegular'

    &-resultBox
      border:1px solid red
      flex: 0 0 857px
      height: 828px
      margin-left:20px
      display: flex
      flex-direction: row
      justify-content: flex-start
      flex-wrap: wrap
      border-top: 1px solid #E1E1E1;
      padding-top:35px
